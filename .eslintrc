{
	"extends": [ "plugin:@wordpress/eslint-plugin/recommended" ],
	"plugins": [ "import" ],
	"rules": {
		"import/no-unresolved": "off" // Disable the import/no-unresolved rule
	},
	"settings": {
		"import/resolver": {
			"node": {
				"extensions": [ ".js", ".jsx" ],
				"paths": [ "./src" ]
			}
		}
	}
}
