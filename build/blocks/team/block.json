{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "crowdify/team",
  "version": "0.1.0",
  "title": "Team Member",
  "category": "crowdify",
  "icon": "smiley",
  "description": "Add a team box with name, position, description and social media links.",
  "example": {},
  "supports": {
    "anchor": true,
    "align": false,
    "html": false,
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "__experimentalFontStyle": true,
      "__experimentalTextTransform": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalDefaultControls": {
        "fontSize": true
      }
    },
    "color": {
      "text": true,
      "gradients": true,
      "background": true,
      "__experimentalDefaultControls": {
        "text": true,
        "background": true
      }
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "__experimentalDefaultControls": {
        "margin": true,
        "padding": true
      }
    }
  },
  "textdomain": "crowdify-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}