{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "crowdify/icon",
  "version": "0.1.0",
  "title": "Icon",
  "category": "crowdify",
  "description": "",
  "example": {},
  "supports": {
    "anchor": true,
    "align": true,
    "html": false,
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true,
      "__experimentalSelector": ".crowdify-icon",
      "__experimentalSkipSerialization": true,
      "__experimentalDefaultControls": {
        "color": true,
        "radius": true,
        "style": true,
        "width": true
      }
    },
    "color": {
      "text": true,
      "background": true,
      "gradients": true,
      "enableContrastChecker": true
    },
    "spacing": {
      "padding": true,
      "margin": true,
      "__experimentalDefaultControls": {
        "margin": false,
        "padding": true
      }
    }
  },
  "attributes": {
    "icon": {
      "type": "string",
      "source": "html",
      "selector": ".crowdify-icon",
      "default": ""
    },
    "iconName": {
      "type": "string"
    },
    "width": {
      "type": "number"
    },
    "height": {
      "type": "number"
    },
    "justifyContent": {
      "type": "string"
    }
  },
  "textdomain": "crowdify-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}